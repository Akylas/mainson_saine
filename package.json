    "pandroid": "cross-var tns build android --bundle --release --keyStorePath $KEYSTORE_PATH --keyStorePassword $KEYSTORE_PASS --keyStoreAlias $KEYSTORE_ALIAS --keyStoreAliasPassword $KEYSTORE_PASS --env.uglify --env.snapshot --env.production",
    "randroid": "cross-var tns run android --bundle --release --keyStorePath $KEYSTORE_PATH --keyStorePassword $KEYSTORE_PASS --keyStoreAlias $KEYSTORE_ALIAS --keyStoreAliasPassword $KEYSTORE_PASS --env.snapshot --env.production",
    "pios": "tns build ios --bundle --release --env.uglify --env.production --for-device --provision 'Maison Saine'"
  },
